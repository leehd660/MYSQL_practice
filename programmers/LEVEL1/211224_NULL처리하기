//IF문 사용해서 문제 해결한 코드
SELECT ANIMAL_TYPE, IF(NAME IS NULL, 'No name', NAME) AS 'NAME', SEX_UPON_INTAKE FROM ANIMAL_INS ORDER BY ANIMAL_ID ASC;

//IFNULL문 사용해서 문제 해결한 코드
SELECT ANIMAL_TYPE, IFNULL(NAME,'No name'), SEX_UPON_INTAKE FROM ANIMAL_INS ORDER BY ANIMAL_ID ASC;

//CASE문 사용해서 문제 해결한 코드
SELECT ANIMAL_TYPE, CASE WHEN(NAME IS NULL) THEN 'No name' ELSE (NAME) END, SEX_UPON_INTAKE FROM ANIMAL_INS ORDER BY ANIMAL_ID ASC;